{
  "expo": {
    "name": "invoice-me",
    "slug": "invoice-me",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c1c1c"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.archlife.teamfront.invoiceme"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1c1c1c"
      },
      "package": "org.archlife.teamfront.invoiceme"
    },
    "web": {
      "favicon": "./assets/icon.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://teamfront-invoice-me-frontend.s3-website-us-west-1.amazonaws.com"
        }
      ],
      "expo-dev-client"
    ],
    "scheme": "invoice-me",
    "extra": {
      "EXPO_PUBLIC_COGNITO_USER_POOL_ID": "us-west-1_5kNwPlpbx",
      "EXPO_PUBLIC_COGNITO_CLIENT_ID": "3ms71cslp7obf865633fsfb11g",
      "EXPO_PUBLIC_COGNITO_REGION": "us-west-1"
    }
  }
}
