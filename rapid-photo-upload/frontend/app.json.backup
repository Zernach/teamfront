{
  "expo": {
    "name": "rapid-photo-upload",
    "slug": "rapid-photo-upload",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c1c1c"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.archlife.teamfront.rapidphotoupload",
      "backgroundColor": "#1c1c1c",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload photos.",
        "NSCameraUsageDescription": "This app needs access to your camera to take photos for upload."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1c1c1c"
      },
      "package": "org.archlife.teamfront.rapidphotoupload",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/icon.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "http://teamfront-rapid-photo-upload-frontend.s3-website-us-west-1.amazonaws.com"
        }
      ],
      "expo-dev-client",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload them.",
          "cameraPermission": "The app accesses your camera to let you take photos for upload."
        }
      ]
    ],
    "scheme": "rapid-photo-upload",
    "extra": {
      "EXPO_PUBLIC_COGNITO_USER_POOL_ID": "us-west-1_M1JfYsJSn",
      "EXPO_PUBLIC_COGNITO_CLIENT_ID": "2hstosrgndta1m2pud1l5dueqf",
      "EXPO_PUBLIC_COGNITO_REGION": "us-west-1"
    }
  }
}
