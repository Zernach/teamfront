{
  "expo": {
    "name": "smart-scheduler",
    "slug": "smart-scheduler",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c1c1c"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.archlife.teamfront.smartscheduler"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1c1c1c"
      },
      "package": "org.archlife.teamfront.smartscheduler"
    },
    "web": {
      "favicon": "./assets/icon.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "http://teamfront-smart-scheduler-frontend.s3-website-us-west-1.amazonaws.com"
        }
      ],
      "expo-dev-client"
    ],
    "scheme": "smart-scheduler"
  }
}
